{
  "name": "@word-wizard/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:app:dev": "nx run word-wizard:build --configuration=development",
    "start:app:dev": "nx run word-wizard:serve --configuration=development",
    "build:app:prod": "nx run word-wizard:build --configuration=production",
    "start:app:prod": "nx run word-wizard:serve --configuration=production",
    "start:api": "uvicorn libs.api.api:app --reload",
    "test:api": "python3 libs/api/run_tests.py",
    "test:e2e": "nx run word-wizard-e2e:e2e",
    "test": "nx run-many --target=test --all --skip-nx-cache --parallel=5 --coverage",
    "test:ci": "nx run-many --target=test --all --skip-nx-cache --parallel=5 --configuration=ci",
    "lint": "nx run-many --target=lint --all --skip-nx-cache --parallel=5",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,scss}\"",
    "check-format": "prettier --check \"**/*.{js,jsx,ts,tsx,html,scss}\"",
    "analytics": "npx unlighthouse --site localhost:4200"
  },
